{
  "name": "data-manager-calendars-option-activity",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data/manager/calendars/option-activity/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/data/manager/calendars/option-activity",
        "main": "libs/data/manager/calendars/option-activity/src/index.ts",
        "tsConfig": "libs/data/manager/calendars/option-activity/tsconfig.lib.json",
        "assets": [
          "libs/data/manager/calendars/option-activity/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/data/manager/calendars/option-activity/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/data/manager/calendars/option-activity"
      ],
      "options": {
        "jestConfig": "libs/data/manager/calendars/option-activity/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
